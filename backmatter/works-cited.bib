@article{caseyDevelopingPhenomenologicalApproach2015,
  title = {Developing a {{Phenomenological Approach}} to {{Music Notation}}},
  author = {Casey, Rob},
  date = {2015-08},
  journaltitle = {Organised Sound},
  shortjournal = {Org. Sound},
  volume = {20},
  pages = {160--170},
  issn = {1355-7718, 1469-8153},
  doi = {10.1017/S1355771815000047},
  url = {https://www.cambridge.org/core/product/identifier/S1355771815000047/type/journal_article},
  urldate = {2021-02-22},
  abstract = {Sound art theorists Seth Kim-Cohen and Salomé Voegelin regard the fixed conceptual structures of notation either as an obstacle to pure sensorial engagement with sound (Voegelin 2010), or as the site of arrogant musical exceptionalism (Kim-Cohen 2009). While sound, whether constituted in phenomenological or idealist terms, is evolving and dynamic, notation is characterised by its ossifying imperative (Kim-Cohen 2009; Voegelin 2010). For Voegelin, a music score is regarded as conceptual, not perceptual. It is read as text and, it seems, has no meaningful place within a phenomenological practice of sound art (Voegelin 2010). The criticism that Vogelin’s phenomenalism, in particular, levels at notation invites close examination of notational practice and the semiotic structures that underwrite it. In this article, I seek to challenge the conceptual imperative of fixed notation through the presentation of a case study in the form of an original composition for string quartet and tape. Drawing on research by Rudolf Arnheim and Mark Johnson, a form of notation will be proposed that enables the score to escape singularly semiotic structures so that it may address the dynamic, phenomenological mode of experience that recent theories of sound art imply is beyond the reach of musical notation.},
  file = {C\:\\Users\\rhys\\Zotero\\storage\\7TENFNSP\\Casey - 2015 - Developing a Phenomenological Approach to Music No.pdf},
  langid = {english},
  number = {2}
}

