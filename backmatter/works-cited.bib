@article{santaellaSoundMusicDomain2015,
  title = {Sound and {{Music}} in the {{Domain}} of {{Rhematic Iconic Qualisigns}}},
  author = {Santaella, Lucia},
  date = {2015-12-31},
  journaltitle = {Signata. Annales des sémiotiques / Annals of Semiotics},
  pages = {91--106},
  publisher = {{Université de liège}},
  issn = {2032-9806},
  doi = {10.4000/signata.1065},
  url = {http://journals.openedition.org/signata/1065},
  urldate = {2021-03-03},
  abstract = {Dans un travail précédent, j’ai développé le raisonnement selon lequel il y a trois matrices de systèmes de signes et de pensée : sonore, visuel et verbal. C’est à partir d’elles que l’on trouve une pluralité de textes et de systèmes mixtes et dérivés. Cette théorie est fondée sur la phénoménologie et la sémiotique de C.S. Peirce. La base de ces trois matrices est constituée par les trois classes de signes principaux de la typologie peircienne : le qualisigne iconique rhématique, le sinsigne indexical dicentique et le légisigne symbolique argumentatif. La théorie postule que les processus sémiotiques sonores sont à discuter principalement à la lumière de la première de ces triades. Les signes visuels appartiennent à la deuxième et les signes verbaux à la troisième triade. Ces classes sont des constructions abstraites, qui nous aident à comprendre les combinaisons réelles des signes existants. Ce principe vaut aussi pour les deux autres matrices. La prépondérance de la première triade pour la musique ne signifie pas que la musique ne présente pas, quant à elle, de signes mixtes. Au contraire. Cependant, cela n’implique pas l’absence d’un centre de rayonnement, dont la fonction est de définir, par la domination d’une classe de signes sur les autres, la constitution du caractère sémiotique de la musique comme système de signes spécifiques. Ce travail cherche à discuter ces aspects de la musique.},
  isbn = {9782875620873},
  issue = {6},
  langid = {english},
  number = {6}
}

